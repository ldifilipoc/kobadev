COLUMN TABLE FT_IMPUESTO(
	CD_DW_BKPF BIGINT NOT NULL,	  -- BELNR NUMERO DOCUMENTO 
	CD_ITEM SMALLINT NOT NULL,	  -- BUZEI ITEM
	CD_IMPUESTO NVARCHAR(2) NOT NULL,
	CD_GIMPUESTO NVARCHAR(3),
	VL_DEBITO DECIMAL(15, 2) ,
	VL_CREDITO DECIMAL(15, 2) ,
	CD_DW_BP INTEGER ,
	CD_DW_CUENTA SMALLINT ,
	PC_TASA DECIMAL(7, 4) ,
	CD_CALC NVARCHAR(2),
	FL_IMPUESTO NVARCHAR(1),
	PRIMARY KEY (
		CD_DW_BKPF,
		CD_ITEM,
		CD_IMPUESTO,
		FL_IMPUESTO
	)
)
UNLOAD PRIORITY 5 AUTO MERGE;
