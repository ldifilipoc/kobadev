 COLUMN TABLE FT_KPI(
 	FC_PERIODO DATE,
 	CD_VERSION SMALLINT,
	CD_DW_CEBE SMALLINT ,
	CD_DW_CECO SMALLINT ,
	CD_DW_MATERIAL INTEGER ,
	CD_DW_CUENTA SMALLINT,
	CD_MONEDA VARCHAR(3),
	VL_KPI DECIMAL(18, 2) ,
	CD_UNIDAD VARCHAR(3),
	VL_CANTIDAD DECIMAL(12,4) ,
	TIMESTMP TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
	NM_USUARIO NVARCHAR(50),
PRIMARY KEY(FC_PERIODO,CD_DW_CEBE,CD_DW_MATERIAL,CD_DW_CUENTA, CD_VERSION)
)
