PROCEDURE "SP_PTD_CARGAR_KPI"(pCuenta VARCHAR(30), pCECO VARCHAR(30), pMes VARCHAR(10), pValor Decimal(16,2))
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   AS
BEGIN

	INSERT INTO STG_MONTHLY_KPI ("PERIODO","KPI","CECO","VERSION","VALOR")
	VALUES (:pMes, :pCuenta, :pCECO,'REAL',:pValor);

END



