			'FROM KOBA_HDI_DB_1.VT_ABAP_ANLC WHERE GJAHR='''||:pEJERCICIO||''' AND AFABE=''01'' AND BUKRS=''6A00'' ' ;
	EXECUTE IMMEDIATE V_SQL;
