    --DELETE FROM STG_ABAP_ANEP WHERE GJAHR=:pEJERCICIO;
 
    V_SQL = 'INSERT INTO KOBA_HDI_DB_1.STG_ABAP_ANEP(MANDT,BUKRS,ANLN1,ANLN2,GJAHR,LNRAN,AFABE,ZUJHR,ZUCOD,PERAF,BELNR,BUZEI,BZDAT,BWASL,ANBTR) '||
