VIEW KOBA_INDICADORES_MONTHLY AS
/*
	Lista de Indicadores compartidos con VALOREM
	
*/
SELECT CD_CUENTA, NM_CUENTA, CD_TIPO_CUENTA, CD_CONCEPTO, NM_CONCEPTO, CD_JERARQUIA_KOBA1 , NM_JERARQUIA_KOBA1 , CD_JERARQUIA_KOBA2 , NM_JERARQUIA_KOBA2 , CD_JERARQUIA_KOBA3 , NM_JERARQUIA_KOBA3 , CD_JERARQUIA_KOBA4 , NM_JERARQUIA_KOBA4 , TIPO_INFORME 
FROM DIM_CUENTA 
WHERE (NOT CD_CONCEPTO  IS NULL  AND NOT CD_CONCEPTO = 'N.A'  AND NOT CD_CONCEPTO = 'KPI9999');

