COLUMN TABLE DIM_FISPER(
	BUKRS NVARCHAR(4),
	PERIODO1 VARCHAR(7) NOT NULL,
	PERIODO2 VARCHAR(7) NOT NULL,
	CD_PERIODO1 VARCHAR(6) NOT NULL DEFAULT '',
	CD_PERIODO2 VARCHAR(6)  NOT NULL DEFAULT '',
	DESDE1 DATE ,
	HASTA1 DATE ,
	DESDE2 DATE ,
	HASTA2 DATE ,
	TIMESTMP TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
	PRIMARY KEY (
		BUKRS
	)
)
UNLOAD PRIORITY 5 AUTO MERGE;
