 COLUMN TABLE DIM_EMPRESA(
	CD_DW_EMPRESA SMALLINT  NOT NULL,
	CD_EMPRESA VARCHAR(4) NOT NULL,
	NM_EMPRESA NVARCHAR(100) NOT NULL,
	NM_GRUPO NVARCHAR(100),
	CD_ENTIDAD NVARCHAR(7),
	CD_MONEDA VARCHAR(3),
	CD_PERIODO_ABIERTO_CONSOLIDACION VARCHAR(7),
	CD_LIBROMAYOR VARCHAR(4),
	PRIMARY KEY (
		CD_DW_EMPRESA
	)
)
UNLOAD PRIORITY 5 AUTO MERGE;
